; ACME 0.90
; CRAZY CYCLES
; PART II - Split Screen (sinus) LORES ou Text / Hires
; version 0.03

!sl "part2label.a"		; sauvegarde labels (debug)

		*= $4000 		; ORG = $4000

; CONSTANTES SYSTEME ===================
VERTBLANK	 = $C019	; 

; Page ZERO
bMachine 		= $0A	; byte MACHINE ($7F/IIE | $FF/GS)
bRefresh		= $0B	; refresh rate : type NTSC/60Hz - PAL/50Hz (00 = 50HZ | 01 = 60 HZ)
; ---------------------------------------
ADR1			= $18   ; +$19
ADR3			= $1C	; +$1D

NextJMP			= $EB	; +$EC
TableEffectLo	= $ED	; +$EE

TableEffectHi	= $F9	; +$FA

PosEffet		= $FC
IndexEffet		= $FD

COUNT			= $FE
Temp			= $FF
; =============================================================================

PARTII	; init sur le premier EFFET + divers
!zone		
; quand on arrive ici depuis MAIN, on est au début de la première ligne de VBL !

; initialisation pendant premiere ligne de VBL
		LDA #$C0				; 2	
		STA ADR1+1				; 3
		STA ADR3+1				; 3

		LDA #00					; 2
		STA COUNT				; 3
		STA IndexEffet			; 3
		STA PosEffet			; 3
		TAX						; 2

		LDA AddrEffetLoL,X		; 4
 		STA TableEffectLo		; 3
	 	LDA AddrEffetLoH,X		; 4
 		STA TableEffectLo+1		; 3
		LDA AddrEffetHiL,X		; 4
 		STA TableEffectHi		; 3
		LDA AddrEffetHiH,X		; 4
 		STA TableEffectHi+1		; 3
 		
		LDA TableMode1,X		; 4
 		STA ADR1				; 3		
		LDA TableMode3,X		; 4
 		STA ADR3				; 3
		
		NOP						; 2
								; = 65 
; seconde ligne VBL
		LDX #11
.bw0	DEX
		BNE .bw0				; 56
		NOP						; 2
								; = 58 cycles au lieu de 65 pour resynchroniser bord gauche (bug ?!)
		
; on arrive début 3eme ligne de VBL
; --------------------------------------------
; suivant PAL OU NTSC
; 1 ligne VBL (NTSC) ou 1 + 50 ligne VBL (PAL)
		LDA bRefresh	; 3
		BNE .NTSC		; 3 / (2)
		
		NOP
		NOP			; 3 (appel routine WAIT) + 1 (compensé BNE non pris)
		LDY #50
		JSR WAIT 	; 50 * 65 (PAL)

.NTSC	; 65 - 6 cycles = 59
		LDX #11
.BW0	DEX
		BNE .BW0 ; 56
		LDA Temp ; 3 
; --------------------------------------------
; début 4ème ligne de VBL
		LDA Temp
		LDY #66 		; 70 - 4 ; [ 3 lignes de VBL déjà effectuées + 1 ligne de VBL à ne pas faire]
		JSR WAIT
; on est maintenant début dernière ligne de VBL
; =======================================================
; debut synchronisation - premier octet dernière ligne de VBL
; =======================================================
BP
!zone
; affichage
		LDY PosEffet				; 3
		LDA (TableEffectLo),Y		; 5
		STA NextJMP					; 3
		LDA (TableEffectHi),Y		; 5
		STA NextJMP+1				; 3
		INY							; 2 = 21

		LDX #6						; important aussi pour la remise à 0 de X
.BW1	DEX
		BNE .BW1
									; 31


		INC Temp					; 5
		LDA Temp					; 3

		JMP (NextJMP)				; 5 	
									; 21 + 31 + 8 + 5 = 65

									; => 65 

; -------------------------------------
; display... (192 lignes)
; -------------------------------------
BACK			; début VBL
!zone	
		
		; ===>
		; **>
		; ->
		INC PosEffet	; 5
		INC PosEffet	; 5
		INC PosEffet	; 5    
		LDA #00			; 2
		STA COUNT		; 3

		LDA $C000		; 4		clavier
		EOR $C061		; 4		bouton 1
		BPL .suite		; 3
		RTS				; sortie !
		; <-			= 31

.suite	; ->
		; compteur + 1
		INC CompLo			; 6
		BNE .s1				; 2/3
		INC CompHi			; 6
		JMP .s1b			; 3
.s1		LDA Temp			; 3
		INC Temp			; 5
 		; <- 			= 17
		
		; <**				; 31 + 17 = 48 !!!
		
		; **>
		; ->
.s1b	LDA CompLo			; 4
		CMP MaxLo			; 4
		BEQ .s2				; 2/3
		; 40 - 10 - 3 = 27
		SBC (Temp,X)		; 6
		SBC (Temp,X)		; 6
		SBC (Temp,X)		; 6
		SBC (Temp,X)		; 6
		LDA Temp			; 3			
		JMP .s5				; 3

		; =>
.s2		LDA CompHi			; 4
		CMP MaxHi			; 4
		BEQ .s3				; 2/3
		; 18+1
		SBC (Temp,X)		; 6
		SBC (Temp,X)		; 6
		NOP					; 2
		NOP					; 2
		JMP .s5				; 3
.s3		
		; index Effet + 1
		; -> 
		LDA #00				; 2
		STA CompLo			; 4
 		STA CompHi			; 4
		INC IndexEffet		; 5
		JMP .s5				; 3
		; <- 				; = 18
		; <= 18 + 11 = 29
		; <**				; 40 !!!

 		; lecture indexEffet
		; **>
.s5		LDX IndexEffet					; 3 <=
		CPX #(FinTableMode1-TableMode1)	; 2
		BNE .s6							; 3 
		; ---
		RTS
		; ---
.s6		
		LDA TableColor,X		; 4
 		JSR FillGR				; 6
		LDA AddrEffetLoL,X		; 4
 		STA TableEffectLo		; 3
	 	LDA AddrEffetLoH,X		; 4
 		STA TableEffectLo+1		; 3
		LDA AddrEffetHiL,X		; 4
 		STA TableEffectHi		; 3
		LDA AddrEffetHiH,X		; 4
 		STA TableEffectHi+1		; 3
 		
		LDA TableMode1,X		; 4
 		STA ADR1				; 3
		LDA TableMode3,X		; 4
 		STA ADR3				; 3

		LDA TableXMode,X		; 4
		STA .offmod+1			; 4
.offmod LDA $C050				; 4		; changement de mode
 		
		LDA TableCompteurLo,X	; 4
 		STA MaxLo				; 4
		LDA TableCompteurHi,X	; 4
 		STA MaxHi				; 4 => 88
								
								; + 3846

		; <**					; = 3934

		; <===== 48 + 40 + 3934 = 4022
		; 65 * 69 = 4485 (on doit revenir à la dernière ligne du VBL donc 69 "lignes")
		; - 4022 = 463

		; 
		; 463/65 = 7 + (8) 

; --------------------------------------------
; suivant PAL OU NTSC => 65*1 ou 65*50 + 65*1
; 1 ligne VBL (NTSC) ou 1 + 50 ligne VBL (PAL)
		LDA bRefresh	; 3
		BNE .NTSC		; 4 / (2) - attention alignement fucked ici BNE = 4 cycles si pris !
		
		NOP				; + 2 pour compenser le BNE non pris (4-2)
		LDA Temp		; appel routine WAIT (11 cycles)
		LDY #50
		JSR WAIT 	; 50 * 65 (PAL)

.NTSC	; 65 - 7 cycles = 58
		LDX #11
.BW0	DEX
		BNE .BW0 ; 56
		NOP		 ; 2
; --------------------------------------------
; 6 lignes VBL
		LDA Temp
		LDY #6			; 6*65
		JSR WAIT
; + 8 cycles		
		INC Temp		; 5
		JMP BP			; 3 
						; = 8 cycles

